class Student:
    def __init__(self, name, age, year_of_birth):
        self.name = name
        self.age = age
        self.year_of_birth = year_of_birth
        self.birth_year_calculated = self.calculate_birth_year()

    def calculate_birth_year(self):
        return 2024 - self.age

student1 = Student("Аня", 19, 2005)

print("Ім'я:", student1.name)
print("Вік:", student1.age)
print("Рік народження:", student1.year_of_birth)
print("Рік народження, обчислений:", student1.birth_year_calculated)
